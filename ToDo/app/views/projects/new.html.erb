<h1>Create Project</h1>
<%= form_for(@project) do |f| %>
<%= f.label :title, "Project Title:" %>
<%= f.text_field :title %>
<br><br>
<%= f.label :description, "Description:" %>
<br>
<%= f.text_area :description %>
<br><br>
<%= f.label :team_id, "Assign to Team:" %>
<%= f.collection_select :team_id, Team.all, :id, :name, { :selected => @passed_team }%>
<br>
<br>
<%= f.submit %>
<% end %>